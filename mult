def mults(*args):
	mult =1
	for i in args:
		mult*=i
	return mult
	
def greatest(list_of_numbers):
    b = 0
    for i in list_of_numbers:
        if i>b:
            b=i
    return b
    
def smallest(list_of_numbers):
    for i in list_of_numbers:
        a= sorted(list_of_numbers)
        i =a[0]
        return i
    
